{
    "manifest_version": 2,
    "name": "Ecommerse Scam Detector",
    "version": "1.0",
    "background": {
        "scripts": [
            "./node_modules/webextension-polyfill/dist/browser-polyfill.js",
            "background.js"
        ]
    },
    "permissions": [
        "webNavigation",
        "tabs"
    ],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png"
    }
}